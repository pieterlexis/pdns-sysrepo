module dns-common-typedef-test {
    yang-version 1.1;
    namespace "http://powerdns.com/yang/dns-common-typedef-test";
    prefix dns-common-typedef-test;

    import dns-common {
        prefix dnsc;
    }


    organization
      "PowerDNS.COM BV";
    contact
      "Pieter Lexis <mailto:pieter.lexis@powerdns.com>";

    description
      "This module is only used for testing the typedefs in dns-common";

    revision 2020-01-23 {
      description "initial";
    }

    container hostnames {
        leaf-list hostname {
            type dnsc:host-name;
        }
    }

    container zonenames {
        leaf-list zonename {
            type dnsc:zone-name;
        }
    }

    container ownernames {
        leaf-list owner-name {
            type dnsc:owner-name;
        }
    }
}